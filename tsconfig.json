{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/components/pie.js", "app/components/subject/1.tsx", "app/components/timer.jsx", "middleware.ts", "app/signin/page.js", "app/signup/page.js", "app/login/page.js", "config.ts", "pages/test/[subject].js", "pages/login.js", "pages/signup.js"],
  "exclude": ["node_modules"]
}
